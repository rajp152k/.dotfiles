{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg": "#0a0a0a",
    "bgPanel": "#151515",
    "bgElement": "#1a1a1a",
    "bgHighlight": "#202020",
    "bgDiffAdded": "#002200",
    "bgDiffRemoved": "#220000",
    "bgDiffAddedHighlight": "#003300",
    "bgDiffRemovedHighlight": "#330000",

    "green": "#00CC00",
    "brightGreen": "#00FF00",
    "springGreen": "#00FA9A",
    "paleGreen": "#98FB98",
    "darkGreen": "#006600",
    "forestGreen": "#228B22",
    "mediumGreen": "#00CD00",

    "slateBlue": "#6A5ACD",
    "lightSlateBlue": "#8470FF",
    "darkSlateBlue": "#483D8B",

    "purple": "#551A8B",
    "darkOrchid": "#9932CC",
    "purpleDeep": "#663399",

    "cyan": "#00FFFF",
    "cyanDark": "#008B8B",

    "red": "#FF5F5F",
    "redBright": "#FF0000",
    "orange": "#FFAF00",
    "yellow": "#FFD700",

    "white": "#FFFFFF",
    "gray": "#606060",
    "grayDark": "#404040"
  },
  "theme": {
    "primary": {
      "dark": "green",
      "light": "darkGreen"
    },
    "secondary": {
      "dark": "slateBlue",
      "light": "darkSlateBlue"
    },
    "accent": {
      "dark": "lightSlateBlue",
      "light": "slateBlue"
    },
    "error": {
      "dark": "red",
      "light": "redBright"
    },
    "warning": {
      "dark": "orange",
      "light": "orange"
    },
    "success": {
      "dark": "springGreen",
      "light": "forestGreen"
    },
    "info": {
      "dark": "cyan",
      "light": "cyanDark"
    },
    "text": {
      "dark": "green",
      "light": "darkGreen"
    },
    "textMuted": {
      "dark": "gray",
      "light": "grayDark"
    },
    "background": {
      "dark": "bg",
      "light": "#E5E9F0"
    },
    "backgroundPanel": {
      "dark": "bgPanel",
      "light": "#D8DEE9"
    },
    "backgroundElement": {
      "dark": "bgElement",
      "light": "#ECEFF4"
    },
    "border": {
      "dark": "darkGreen",
      "light": "forestGreen"
    },
    "borderActive": {
      "dark": "brightGreen",
      "light": "darkGreen"
    },
    "borderSubtle": {
      "dark": "#333333",
      "light": "#CCCCCC"
    },
    "diffAdded": {
      "dark": "springGreen",
      "light": "forestGreen"
    },
    "diffRemoved": {
      "dark": "red",
      "light": "redBright"
    },
    "diffContext": {
      "dark": "gray",
      "light": "grayDark"
    },
    "diffHunkHeader": {
      "dark": "slateBlue",
      "light": "darkSlateBlue"
    },
    "diffHighlightAdded": {
      "dark": "brightGreen",
      "light": "forestGreen"
    },
    "diffHighlightRemoved": {
      "dark": "redBright",
      "light": "redBright"
    },
    "diffAddedBg": {
      "dark": "bgDiffAdded",
      "light": "#E5F0E5"
    },
    "diffRemovedBg": {
      "dark": "bgDiffRemoved",
      "light": "#F0E5E5"
    },
    "diffContextBg": {
      "dark": "bgPanel",
      "light": "#E5E9F0"
    },
    "diffLineNumber": {
      "dark": "darkGreen",
      "light": "forestGreen"
    },
    "diffAddedLineNumberBg": {
      "dark": "bgDiffAdded",
      "light": "#E5F0E5"
    },
    "diffRemovedLineNumberBg": {
      "dark": "bgDiffRemoved",
      "light": "#F0E5E5"
    },
    "markdownText": {
      "dark": "green",
      "light": "darkGreen"
    },
    "markdownHeading": {
      "dark": "brightGreen",
      "light": "darkGreen"
    },
    "markdownLink": {
      "dark": "lightSlateBlue",
      "light": "slateBlue"
    },
    "markdownLinkText": {
      "dark": "springGreen",
      "light": "forestGreen"
    },
    "markdownCode": {
      "dark": "springGreen",
      "light": "forestGreen"
    },
    "markdownBlockQuote": {
      "dark": "paleGreen",
      "light": "forestGreen"
    },
    "markdownEmph": {
      "dark": "brightGreen",
      "light": "forestGreen"
    },
    "markdownStrong": {
      "dark": "slateBlue",
      "light": "darkSlateBlue"
    },
    "markdownHorizontalRule": {
      "dark": "darkGreen",
      "light": "forestGreen"
    },
    "markdownListItem": {
      "dark": "green",
      "light": "darkGreen"
    },
    "markdownListEnumeration": {
      "dark": "slateBlue",
      "light": "darkSlateBlue"
    },
    "markdownImage": {
      "dark": "lightSlateBlue",
      "light": "slateBlue"
    },
    "markdownImageText": {
      "dark": "springGreen",
      "light": "forestGreen"
    },
    "markdownCodeBlock": {
      "dark": "springGreen",
      "light": "forestGreen"
    },
    "syntaxComment": {
      "dark": "springGreen",
      "light": "forestGreen"
    },
    "syntaxKeyword": {
      "dark": "brightGreen",
      "light": "darkGreen"
    },
    "syntaxFunction": {
      "dark": "green",
      "light": "darkGreen"
    },
    "syntaxVariable": {
      "dark": "paleGreen",
      "light": "forestGreen"
    },
    "syntaxString": {
      "dark": "lightSlateBlue",
      "light": "slateBlue"
    },
    "syntaxNumber": {
      "dark": "cyan",
      "light": "cyanDark"
    },
    "syntaxType": {
      "dark": "slateBlue",
      "light": "darkSlateBlue"
    },
    "syntaxOperator": {
      "dark": "mediumGreen",
      "light": "darkGreen"
    },
    "syntaxPunctuation": {
      "dark": "forestGreen",
      "light": "darkGreen"
    }
  }
}
